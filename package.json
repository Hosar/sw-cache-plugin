{
  "name": "sw-cache-plugin",
  "version": "0.1.0",
  "description": "Webpack plugin that allows to cache generated assets in your on service worker.",
  "main": "lib/index.js",
  "scripts": {
    "test": "DEBUG=test,app ava --verbose",
    "prepublish": "babel src --out-dir lib",
    "build": "cp src/template/sw-cache.tmpl lib/template && babel src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hosar/sw-cache-plugin.git"
  },
  "keywords": [
    "caching",
    "offline",
    "precaching",
    "service-worker",
    "serviceworker",
    "webpack",
    "plugin",
    "sw"
  ],
  "author": {
    "name": "Hosarsiph Valle Hernandez",
    "email": "hosarsiph.valle@gmail.com"    
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hosar/sw-cache-plugin/issues"
  },
  "homepage": "https://github.com/Hosar/sw-cache-plugin#readme",
  "engines": {
    "node": ">=5"
  },
  "dependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-stage-0": "^6.16.0",
    "bluebird": "^3.4.6",
    "chalk": "^1.1.3",
    "git-release-notes": "^0.0.2",
    "lodash.defaults": "^4.2.0",
    "lodash.template": "^4.4.0",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-register": "^6.18.0",
    "nyc": "^10.0.0",
    "sinon": "^1.17.6"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "src/**/*.{js}"
    ],
    "concurrency": 5,
    "failFast": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "verbose": true
  }
}
